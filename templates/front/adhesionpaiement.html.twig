 {% extends ('layout/frontlayout.html.twig') %}
   {% block body %} 
   <section class="inner-header divider parallax layer-overlay overlay-dark-5" data-bg-img="images/bg/bg3.jpg" style="background-image: url(&quot;images/bg/bg3.jpg&quot;); background-position: 50% 97px;">
      <div class="container pt-70 pb-20">
        <!-- Section Content -->
        <div class="section-content">
          <div class="row">
            <div class="col-md-8 col-md-offset-2 text-center">
              <h2 class="text-white mt-20">Adhésion ISFESO®</h2>
            </div>
          </div>
        </div>
      </div>      
    </section>

    <section>
      <div class="container">
        <div class="row">
          <div class="col-md-6 col-md-push-3">
              <div class="icon-box mb-0 p-0">
                <a href="#" class="icon icon-bordered icon-rounded icon-sm pull-left mb-0 mr-10">
                  <i class="pe-7s-users"></i>
                </a>
                <h4 class="text-gray pt-10 mt-0 mb-30">Poursuivre au paiement</h4>
              </div>
              <hr>
              {% for message in app.flashes('success')  %}
                <div class="alert alert-success">
                    {{ message }}
                </div>
              {% endfor %}
			  
            <form action="https://www.mypos.eu/vmp/btn/" method="post" target="_blank">
                <input type="hidden" name="button_id" value="BHGNSRCOIMR67"/>
                <input type="image" src="https://www.mypos.eu/img/vmypos/lg/btn-fr.png" border="0" name="submit" alt="Pay"/>
            </form>
          </div>
        </div>
      </div>
    </section>



   {% endblock %}
